@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" name="viewport"
          content="text/html; charset=UTF-8; width=device-width, initial-scale=1.0">
    <title>List</title>
</head>
<body>
    <!-- Modal -->
    <div class="modal-body">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h3 id="myModalLabel">字段列表</h3>
        </div>
        <div class="modal-body">
            <div id="myAttributeGrid" style="width:100%;height:200px;float:left;"></div>
        </div>
        <div class="modal-footer">
            <button class="btn btn-primary">确定</button>
            <button class="btn" data-dismiss="modal" aria-hidden="true">取消</button>
        </div>
    </div>

    <script type="text/javascript" src="~/ViewJS/attributelist.js"></script>
    <script type="text/javascript">
        $(function () {
            attributelist.load();
        })
    </script>

</body>
</html>
